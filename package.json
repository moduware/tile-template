{
  "name": "pwa-starter-kit",
  "version": "0.9.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "scripts": {
    "start": "polymer serve",
    "start:webpack": "webpack-dev-server --open --mode development",
    "build": "polymer build",
    "build:static": "polymer build",
    "build:webpack": "rimraf dist && webpack --config webpack.config.js",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "serve:webpack": "polymer serve --port 3000 dist",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm",
    "test:sauce": "polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "lit-translate": "^1.1.20",
    "@appnest/lit-translate": "^1.1.16",
    "@polymer/app-layout": "^3.0.0",
    "@polymer/polymer": "^3.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "lit-element": "^2.0.1",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "webview-tile-header": "git+https://github.com/moduware/webview-tile-header.git#lit"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.0.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^3.4.2",
    "del": "^3.0.0",
    "file-loader": "^5.0.2",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.11",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.0",
    "puppeteer": "^1.5.0",
    "rimraf": "^3.0.1",
    "style-loader": "^1.1.3",
    "wct-mocha": "^1.0.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2",
    "workbox-webpack-plugin": "^3.6.3"
  }
}
